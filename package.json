{
  "name": "task",
  "version": "0.0.1",
  "description": "task scheduler for the browser and node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/task.git"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
    "test-node-raw": "node ./test",
    "test-browser-local": "zuul --local 9999 --ui tape -- ./test"
  },
  "keywords": [
    "task",
    "task.js"
  ],
  "author": "Nathan Faucett",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-jsbeautifier": "^0.2.10",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2",
    "zuul": "^3.7.2"
  },
  "dependencies": {
    "array-for_each": "git://github.com/nathanfaucett/array-for_each.git",
    "async_done": "git://github.com/nathanfaucett/async_done.git",
    "has": "git://github.com/nathanfaucett/has.git",
    "is_function": "git://github.com/nathanfaucett/is_function.git",
    "is_string": "git://github.com/nathanfaucett/is_string.git",
    "once": "git://github.com/nathanfaucett/once.git"
  }
